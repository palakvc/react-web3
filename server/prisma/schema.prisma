// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:nestDb.db"
}

model User {
  id Int @default(autoincrement()) @id
  account_id String @unique
  hash String @unique
  username String
  full_name String
  email String
  bio String
  profileImage String
  coverImage String
  accessToken String
}